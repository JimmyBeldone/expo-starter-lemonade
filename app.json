{
    "expo": {
        "name": "expo-starter-lemonade",
        "slug": "expo-starter-lemonade",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icons/icon.png",
        "splash": {
            "image": "./src/assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/icons/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./src/assets/images/icons/favicon.png"
        },
        "packagerOpts": {
            "sourceExts": ["js", "json", "jsx", "svg", "ts", "tsx"]
        }
    }
}
